{
  "env": {
    "commonjs": {
      "presets": ["env"]
    },
    "es": {
      "presets": [
        ["env", {
          "modules": false
        }]
      ]
    },
    "umd": {
      "presets": ["env"],
      "plugins": [
        "add-module-exports"
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "modules": "commonjs"
        }]
      ]
    }
  }
}
